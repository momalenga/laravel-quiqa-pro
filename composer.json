{
    "name": "shengamo/laravel-quiqa-pro",
    "description": "A plugin to access the quiqa pro app in Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mo Malenga",
            "email": "mo@shengamo.com",
            "homepage": "www.shengamo.com"
        }
    ],
    "homepage": "https://github.com/shengamo/laravel-quiqa-pro",
    "keywords": ["Laravel", "LaravelQuiqaPro","Quiqa"],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Shengamo\\LaravelQuiqaPro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shengamo\\LaravelQuiqaPro\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shengamo\\LaravelQuiqaPro\\LaravelQuiqaProServiceProvider"
            ],
            "aliases": {
                "LaravelQuiqaPro": "Shengamo\\LaravelQuiqaPro\\Facades\\LaravelQuiqaPro"
            }
        }
    }
}
